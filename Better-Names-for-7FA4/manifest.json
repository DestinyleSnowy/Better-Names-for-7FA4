{
  "manifest_version": 3,
  "name": "Better Names for 7FA4",
  "version": "6.0.0.18",
  "description": "强大的 7FA4 用户名与颜色映射插件",
  "permissions": [
    "storage",
    "notifications",
    "clipboardWrite",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://7fa4.cn:8888/*",
    "http://*.7fa4.cn:8888/*",
    "http://7fa4.cn:5283/*",
    "http://*.7fa4.cn:5283/*",
    "http://10.210.57.10:8888/",
    "http://211.137.101.118:8888/",
    "http://10.210.57.10:5283/",
    "http://211.137.101.118:5283/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://7fa4.cn:8888/*",
        "http://*.7fa4.cn:8888/*",
        "http://7fa4.cn:5283/*",
        "http://*.7fa4.cn:5283/*",
        "http://10.210.57.10:8888/",
        "http://211.137.101.118:8888/",
        "http://10.210.57.10:5283/",
        "http://211.137.101.118:5283/"
      ],
      "exclude_matches": [
        "http://7fa4.cn:9080/*",
        "http://*.7fa4.cn:9080/*",
        "http://10.210.57.10:9080/",
        "http://211.137.101.118:9080/"
      ],
      "css": [
        "content/panel/panel.css"
      ],
      "js": [
        "content/gm-shim.js",
        "content/panel/panel.js",
        "content/main.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "data/users.json",
        "content/panel/panel.html"
      ],
      "matches": [
        "http://7fa4.cn:8888/*",
        "http://*.7fa4.cn:8888/*",
        "http://7fa4.cn:5283/*",
        "http://*.7fa4.cn:5283/*",
        "http://10.210.57.10:8888/*",
        "http://211.137.101.118:8888/*",
        "http://10.210.57.10:5283/*",
        "http://211.137.101.118:5283/*"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "submitter/popup.html"
  }
}
