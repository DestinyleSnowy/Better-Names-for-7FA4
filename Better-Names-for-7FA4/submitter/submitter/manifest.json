{
	"name": "7FA4 submitter",
	"version": "0.1.4",
	"description": "Submit code from foreign OJ's",
	"manifest_version": 3,
	"permissions": [
		"notifications",
		"storage",
		"activeTab",
		"scripting"
	],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"jquery-3.7.0.min.js",
				"jquery.cookie.js"
			],
			"all_frames": true,
			"run_at": "document_end"
		}
	],
	"host_permissions": ["*://*.7fa4.cn/*", "*://7fa4.cn/*"]
}
